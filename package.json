{
  "name": "mindease",
  "private": true,
  "type": "module",
  "version": "0.1.0",
  "description": "MindEase monorepo",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "engines": {
    "node": ">=18 <=22"
  },
  "scripts": {
    "clean": "npx rimraf \"**/node_modules\" \"**/dist\"",
    "dev": "echo \"dev wired later\" && exit 0",
    "lint": "eslint . --quiet",
    "lint:fix": "eslint . --fix",
    "lint:verbose": "eslint .",
    "format": "prettier . --write",
    "format:check": "prettier . --check",
    "build": "npm run -w @mindease/marketing build && npm run -w @mindease/webapp build",
    "prepare": "husky"
  },
  "devDependencies": {
    "@babel/core": "^7.28.3",
    "@babel/eslint-parser": "^7.28.0",
    "@babel/preset-react": "^7.27.1",
    "@eslint/js": "^9.34.0",
    "@types/node": "24.9.2",
    "@types/react": "19.2.2",
    "@types/react-dom": "19.2.2",
    "eslint": "^9.34.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.3.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.5",
    "postcss": "8.5.6",
    "prettier": "^3.6.2",
    "rimraf": "^6.0.0",
    "tailwindcss": "4.1.16",
    "typescript": "5.9.3"
  },
  "dependencies": {
    "@dr.pogodin/react-helmet": "3.0.4",
    "@stomp/stompjs": "7.2.1",
    "@stripe/stripe-js": "8.2.0",
    "@tailwindcss/postcss": "4.1.16",
    "@testing-library/jest-dom": "6.9.1",
    "@vitejs/plugin-react": "5.1.0",
    "axios": "1.13.1",
    "chart.js": "4.5.1",
    "class-variance-authority": "0.7.1",
    "clsx": "2.1.1",
    "cypress": "15.5.0",
    "firebase": "12.5.0",
    "framer-motion": "12.23.24",
    "i18next": "25.6.0",
    "i18next-browser-languagedetector": "8.2.0",
    "lottie-react": "2.4.1",
    "lucide-react": "0.548.0",
    "next": "16.0.1",
    "react": "19.2.0",
    "react-chartjs-2": "5.3.1",
    "react-dom": "19.2.0",
    "react-i18next": "16.2.3",
    "react-router-dom": "7.9.5",
    "react-toastify": "11.0.5",
    "sockjs-client": "1.6.1",
    "tailwind-merge": "3.3.1",
    "tailwindcss-animate": "1.0.7",
    "vite": "7.1.12",
    "vite-plugin-pwa": "1.1.0",
    "whatwg-fetch": "3.6.20"
  }
}
